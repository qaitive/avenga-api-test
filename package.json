{
  "name": "avenga-api-testing",
  "version": "1.0.0",
  "author": "Veljko Jovancic",
  "description": " API testing project using Axios, Mocha, and Chai",
  "scripts": {
    "test": "npx mocha test/*.js --timeout 10000",
    "report": "npx mocha test/*.js --timeout 10000 --reporter mochawesome",
    "testReport": "npx mocha --reporter mochawesome --reporter-options reportDir=mochawesome-report,reportFilename=index"
  },
    "dependencies": {
    "axios": "^1.7.2",
    "winston": "^3.14.2"
  },
  "devDependencies": {
    "@faker-js/faker": "^9.0.3",
    "chai": "^5.1.1",
    "mocha": "^10.7.3",
    "mochawesome": "^6.2.1"
  }
}
